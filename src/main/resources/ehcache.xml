<?xml version="1.0" encoding="UTF-8"?>
<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:noNamespaceSchemaLocation="http://ehcache.org/ehcache.xsd"
 	updateCheck="false">
 
 <!-- Default Cache Configuration, with name 'default' -->
 <defaultCache maxElementsInMemory="100" eternal="false" overflowToDisk="false" memoryStoreEvictionPolicy="LFU" />
 
 <!-- set up techCache element for eh-cache -->
 <cache name="agencyCache" maxElementsInMemory="100" eternal="false" overflowToDisk="true" memoryStoreEvictionPolicy="LFU" />
 
</ehcache>